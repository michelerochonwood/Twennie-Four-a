{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "twennie.com"
        }
      ],
      "destination": "https://www.twennie.com/:path*",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "twennie-four-a.vercel.app"
        }
      ],
      "destination": "https://www.twennie.com/:path*",
      "permanent": true
    }
  ]
}


