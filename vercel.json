{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "twennie.com" }
      ],
      "destination": "https://www.twennie.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "twennie-four-a.vercel.app" }
      ],
      "destination": "https://www.twennie.com/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(css|images|icons|javascript|badges|public|uploads|promptprogress)/(.*)",
      "destination": "/public/$1/$2"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}





